{
  "name": "ybiq",
  "version": "0.1.0",
  "description": "Command-line utilities for npm",
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "repository": "ybiquitous/ybiq",
  "bugs": "https://github.com/ybiquitous/ybiq/issues",
  "homepage": "https://github.com/ybiquitous/ybiq#readme",
  "keywords": [
    "cli",
    "command-line",
    "utility",
    "tool"
  ],
  "main": "index.js",
  "bin": {
    "ybiq": "./bin/cli.js"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "eslint": "^4.6.1",
    "eslint-config-ybiquitous": "^2.0.2",
    "fs-extra": "^4.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.2",
    "markdownlint-cli": "^0.3.1",
    "npm-run-all": "^4.1.1",
    "standard-version": "^4.2.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@std/esm": "^0.7.1",
    "codecov": "^2.3.0",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1"
  },
  "scripts": {
    "test": "nyc mocha",
    "test:watch": "mocha --watch",
    "test:coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:js": "eslint --max-warnings=0 .",
    "lint:js:fix": "eslint --max-warnings=0 . --fix",
    "lint:md": "markdownlint *.md",
    "lint": "npm-run-all --parallel lint:js lint:md",
    "commitmsg": "commitlint -e",
    "precommit": "lint-staged",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix --max-warnings=0 --no-ignore",
      "git add"
    ],
    "*.md": "markdownlint"
  },
  "@std/esm": {
    "esm": "all"
  }
}
